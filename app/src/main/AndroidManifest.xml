<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.universe_of_software.total_calculator">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.universe_of_software.total_calculator.ListView.itemActivity.ContactUsActivity"></activity>
        <activity android:name="com.universe_of_software.total_calculator.ListView.itemActivity.PatchNoteActivity" />
        <activity android:name="com.universe_of_software.total_calculator.OptionActivity" />
        <activity android:name="com.universe_of_software.total_calculator.MainActivity" />
        <activity android:name="com.universe_of_software.total_calculator.Splash_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>